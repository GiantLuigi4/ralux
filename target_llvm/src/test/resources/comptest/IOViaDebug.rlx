pkg comptest;

use ralux.debug.Debug;

public class IOViaDebug {
    public static int main() {
        Debug.write(5);
        long lng = Debug.readInt64();
        Debug.write(lng);
        if (lng > 3806) {
            Debug.write(1);
        } else if (lng < 50) Debug.write(0);
        else Debug.write(3);

        return 0;
    }
}
